{
  "name": "tree-sitter-plsqloracle",
  "version": "0.1.0",
  "description": "PL/SQL with Oracle flavor",
  "repository": "https://github.com/tree-sitter/tree-sitter-plsqloracle",
  "license": "MIT",
  "author": {
    "name": "Vin√≠cius Sugimoto",
    "email": "vtmsugimoto@gmail.com",
    "url": "https://github.com/vinicius-toshiyu"
  },
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "plsqloracle"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.2.1",
    "node-gyp-build": "^4.8.2"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter-cli": "^0.25.6"
  },
  "peerDependencies": {
    "tree-sitter": "^0.21.1"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "build": "tree-sitter generate && tree-sitter build && rm -r build && npm run install",
    "install": "node-gyp-build && node build-constants.js src/grammar.json",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  },
  "tree-sitter": {
    "scope": "source.plsqloracle",
    "file-types": [
      "plsql",
      "plsqltemplate"
    ],
    "injection-regex": "plsqloracle",
    "highlights": [
      "queries/highlights.scm"
    ]

  }
}
